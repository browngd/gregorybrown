body {
  color: #222;
  background-color: #282727;
}

.border {
  border: 1px solid #ddd;
}

.hide {
  display: none;
}

.platform {
  margin: 20px auto;
  background: #474747;
  -webkit-box-shadow: 0 0px 20px -1px #000000;
  -moz-box-shadow: 0 0px 20px -1px #000000;
  box-shadow: 0 0px 20px -1px #000000;
  border: 0px solid #444;
  border-radius: 15px 15px 15px 15px;
  -moz-border-radius: 15px 15px 15px 15px;
  -webkit-border-radius: 15px 15px 15px 15px;
  -khtml-border-radius: 15px 15px 15px 15px;
/*  background-image: -moz-linear-gradient(center top , #353434, #353434); */
  box-shadow: 0 0 15px rgba(0, 0, 0, 0.85), 0 85px 180px 30px #888888, 0 12px 80px -0px rgba(0, 0, 0, 0.85);
  border: 3px solid #424242;
  border-radius: 20px;
  -moz-border-radius: 15px;
  -webkit-border-radius: 15px;
  -khtml-border-radius: 15px;
}

.header {
  padding-top: 10px;
  padding-left: 50px;
  padding-bottom: 15px;
  padding-right: 50px;
  background: url("bg.png") repeat scroll 0 0 #474747;
  /*-webkit-box-shadow: 0 10px 10px -1px #314E5B; -moz-box-shadow: 0 10px 10px -1px #314E5B; box-shadow: 0 10px 10px -1px #314E5B; */
  border: 0px solid #4E7B91;
  border-radius: 15px 15px 0px 0px;
  -moz-border-radius: 15px 15px 0px 0px;
  -webkit-border-radius: 15px 15px 0px 0px;
  -khtml-border-radius: 15px 15px 0px 0px;
}

.footer {
  padding: 40px;
  padding-bottom: 30px;
  background: url("bg.png") repeat scroll 0 0 #474747;
  text-align: center;
  /* -webkit-box-shadow: 0 10px 10px -1px #314E5B; -moz-box-shadow: 0 10px 10px -1px #314E5B; box-shadow: 0 10px 10px -1px #314E5B; */
  border: 0px solid #4E7B91;
  border-radius: 0px 0px 15px 15px;
  -moz-border-radius: 0px 0px 15px 15px;
  -webkit-border-radius: 0px 0px 15px 15px;
  -khtml-border-radius: 0px 0px 15px 15px;
}

.progress-line {
  background: #B7B3B3;
  margin-bottom: 15px;
  border-radius: 5px 5px 5px 5px;
}

.progress-line .bar {
  color: #eee;
  font-size: 12px;
  text-align: center;
  text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25);
  background-color: #A09D9D;
  background-image: -moz-linear-gradient(top, #525252, #3e3e3e);
  background-image: -ms-linear-gradient(top, #525252, #3e3e3e);
  background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#525252), to(#3e3e3e));
  background-image: -webkit-linear-gradient(top, #525252, #3e3e3e);
  background-image: -o-linear-gradient(top, #525252, #3e3e3e);
  background-image: linear-gradient(top, #525252, #3e3e3e);
  background-repeat: repeat-x;
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#525252', endColorstr='#3e3e3e', GradientType=0);
  -webkit-box-shadow: inset 0 -1px 0 rgba(0, 0, 0, 0.15);
  -moz-box-shadow: inset 0 -1px 0 rgba(0, 0, 0, 0.15);
  box-shadow: inset 0 -1px 0 rgba(0, 0, 0, 0.15);
  border-radius: 3px 3px 3px 3px;
}

.pic-div {
  padding-bottom: 10px;
  padding-right: 20px;
}

.pic {
  filter: url("data:image/svg+xml;utf8,<svg xmlns=\'http://www.w3.org/2000/svg\'><filter id=\'grayscale\'><feColorMatrix type=\'matrix\' values=\'0.3333 0.3333 0.3333 0 0 0.3333 0.3333 0.3333 0 0 0.3333 0.3333 0.3333 0 0 0 0 0 1 0\'/></filter></svg>#grayscale"); /* Firefox 3.5+, IE10 */
  filter: gray; /* IE6-9 */
  -webkit-filter: grayscale(100%); /* Chrome 19+ & Safari 6+ */
  -webkit-transition: all .6s ease; /* Fade to color for Chrome and Safari */
  -webkit-backface-visibility: hidden; /* Fix for transition flickering */
  background-clip: padding-box;
  background-color: white;
  border: 10px solid rgba(0, 0, 0, 0.2);
  border-radius: 20px 20px 20px 20px;
  -moz-border-radius: 20px 20px 20px 20px; -webkit-border-radius: 20px 20px 20px 20px; -khtml-border-radius: 20px 20px 20px 20px;
  background-image: url(../img/gbrown_profile.jpg);
  width: 170px;
  height: 236px;
}

.pic:hover {
    filter: none;
    -webkit-filter: grayscale(0%);
}

