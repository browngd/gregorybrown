.section-title {
  width: 180px;
  font-size: 20px;
  font-style: italic;
  padding: 20px 0px 20px 20px;
  background-color: #474747;
  background: url("bg.png");
  margin-bottom: 15px;
  -moz-box-shadow: 10px 10px 5px #888;
  -webkit-box-shadow: 10px 10px 5px #888;
  box-shadow: 10px 10px 20px 0px #434343;
  text-shadow: 0px 2px 3px #555;
  font: color: #314E5B; text-shadow: 0px 1px 30px #ffffff;
  border:0px solid #474747;
  border-radius:0px 15px 15px 0px;
  -moz-border-radius: 0px 15px 15px 0px;
  -webkit-border-radius: 0px 15px 15px 0px;
  -khtml-border-radius: 0px 15px 15px 0px;

 /* border-right: 5px solid #87AABA; */
 /* border-bottom: 4px solid #3A6477; */
}

.section-body { }
.seperator {border-bottom: 1px solid #434343; margin: 20px 0 30px 0;}
.seperator-line {border-bottom: 0px solid #434343; margin: 2px 0 2px 0;}
.inner-seperator {margin:10px 0 20px 0; border-bottom: 1px dashed #434343;}
.section-body ul {margin-left: 17px;}
.subject-name { font-size: 16px; font-weight: bold; color: #000000; margin-bottom: 5px;}
.download { margin-top: 20px;}
.qr { margin-top: 20px; margin-bottom: 20px;}
.pdf {width: 75px; display:block; height: 75px; background-image: url(../img/pdf.png); cursor: pointer; margin-top:20px; margin-bottom: 20px;}
a.dotted {border-bottom: 1px black dotted; display: block; color: #000000;}
a.dotted:hover {text-decoration: none; color: #A50109;}

/*TILT*/
.tilt {-webkit-transition: all 0.5s ease; -moz-transition: all 0.5s ease; -o-transition: all 0.5s ease; -ms-transition: all 0.5s ease; transition: all 0.5s ease;}
.tilt:hover {-webkit-transform: rotate(-10deg); -moz-transform: rotate(-10deg); -o-transform: rotate(-10deg); -ms-transform: rotate(-10deg); transform: rotate(-10deg);}

/* Portrait tablet to landscape and desktop */
@media (min-width: 900px) {
  .platform {width:980px; }
  .section-title {width: 180px;}
  .section-body { width: 690px; }
  .picture {float:none; text-align:center;}

  .content { padding-left: 20px; padding-right:20px; }

 /* .pdf {position: fixed; top:189px; margin-left: 0px;} */

  .item-name { max-width:550px; font-size: 16px; font-weight: bold; color: #000000;}
  .item-desc { max-width:550px; font-size:14px; font-style: italic;}

}

@media (max-width: 900px) {
  .section-title {width:100%;}
  .content { padding-left: 20px; padding-right:20px; }
  .item-name {font-weight: bold;}
}
/* Landscape phone to portrait tablet */
@media (max-width: 767px) {
  .address {margin-left: 0px;}
  .name-text {margin-top: 10px;}
  .section-title {width: 100%;
  border:0px solid #4E7B91; border-radius:0px 0px 0px 0px; -moz-border-radius: 0px 0px 0px 0px; -webkit-border-radius: 0px 0px 0px 0px; -khtml-border-radius: 0px 0px 0px 0px;

  }
  .pic-div {text-align: center;}
  .pic {position: relative; float:none; text-align:center;}
  .content { padding-left: 20px; padding-right:20px; }
  .item-name {font-weight: bold;}
  .buttons {padding-bottom: 25px; padding-top: 15px;}
/*  .pdf {position: fixed; top:589px; margin-left: 300px;} */

}

/* Landscape phones and down */
@media (max-width: 480px) {
  .name-text {margin-top: 10px;}
  .main {padding: 20px 0px 20px 0px;}
  .platform {margin: 0px auto;}
  .header {padding:10px; padding-bottom:0px;}
  .seperator {margin: 5px 0 10px 0;}
  .inner-seperator {margin:5px 0 10px 0; }
  .address {margin-left: 0px;}
  .section-title {width: 100%;
  border:0px solid #4E7B91; border-radius:0px 0px 0px 0px; -moz-border-radius: 0px 0px 0px 0px; -webkit-border-radius: 0px 0px 0px 0px; -khtml-border-radius: 0px 0px 0px 0px;
  }
  .pic {float:none; text-align:center;}
  .buttons {padding-bottom: 25px; padding-top: 15px;}
  .content { padding-left: 20px; padding-right:20px; }
  .item-name {font-weight: bold;}
/*  .pdf {position: fixed; top:589px; margin-left: 100px;} */

}

.portfolio-item {
    display: inline-block;
    margin-bottom: 12px;
    vertical-align: top;
    width: 150px;
}
.portfolio-item > img {
    margin-bottom: -1px;
    max-width: 100%;
}
.portfolio-details {
    border: 1px solid #D8D8D8;
    padding: 12px 12px 10px;
}
.portfolio-details > h4 {
    color: #74744E;
    font-size: 11px;
    font-weight: 700;
    line-height: normal;
    margin-bottom: 3px;
    text-transform: uppercase;
}
.portfolio-details > p {
    font-size: 11px;
    font-weight: 400;
    line-height: 1.2;
    margin-top: 5px;
}
.portfolio-details > p + img {
    margin-bottom: 5px;
    margin-top: 10px;
}
.portfolio-details > h4 + img {
    margin-bottom: 5px;
    margin-top: 7px;
}
.portfolio-details > img + h4 {
    margin-top: 10px;
}
.portfolio-details a {
    font-size: 12px;
    font-weight: 400;
}
.portfolio-details i {
    font-size: 13px;
    font-weight: normal;
}
.portfolio-links {
    margin-top: 10px !important;
}




